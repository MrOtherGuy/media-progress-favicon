{
  "description": "Adds a progress icon to tab favicon",
	"author": "MrOtherGuy",
  "manifest_version": 2,
  "name": "Progressicon",
  "permissions": ["<all_urls>"],
  "version": "0.2",
	"background": {
		"scripts": ["background-script.js"]
	},
	"browser_action": {
      "default_title": "Toggle Darkmode",
			"browser_style": true
  },
	"applications": {
  "gecko": {
    "id": "progressicon@example.com",
		"strict_min_version": "66.0"
  }
}
}